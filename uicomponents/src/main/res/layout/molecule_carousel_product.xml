        android:layout_height="wrap_content"
        android:id="@+id/imgCarouselProduct"
        />

    <com.active24.uicomponents.atom.AtomicTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:text="TÃ­tulo"
