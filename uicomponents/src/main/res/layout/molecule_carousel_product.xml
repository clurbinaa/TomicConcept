        android:id="@+id/textViewCarouselProduct"
        />

    <android.support.v4.widget.Space
        android:layout_width="match_parent"
        android:layout_height="10dp" />

    <com.active24.uicomponents.atom.AtomicTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:text="precio 1"
        android:id="@+id/textViewPrice1CarouselProduct"
        />

    <com.active24.uicomponents.atom.AtomicTextView
