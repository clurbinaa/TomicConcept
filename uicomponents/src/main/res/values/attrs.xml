<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AtomicTextView">
        <attr name="atom_style" format="enum">
            <enum name="title"            value="1"/>
            <enum name="subtitle1"        value="2"/>
            <enum name="subtitle2"        value="3"/>
            <enum name="body1"            value="4"/>
            <enum name="cash_format"      value="5"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="AtomicButton">
        <attr name="button_style" format="enum">
            <enum name="pink"            value="1"/>
            <enum name="white_next"        value="2"/>
            <enum name="dark_gray"        value="3"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="MoleculePrice">
        <attr name="price_type" format="integer">
            <enum name="normal"            value="1"/>
            <enum name="discount"        value="2"/>
            <enum name="transfer"        value="3"/>
        </attr>
        <attr name="normalPrice" format="string" />
        <attr name="regularPrice" format="string" />
        <attr name="promotionPrice" format="string" />
        <attr name="transferPrice" format="string" />
    </declare-styleable>
</resources>